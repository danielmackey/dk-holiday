a
  text-decoration none

a:visited
  color inherit
  text-decoration none

hr
  height 6px
  border none
  border-top 2px solid #59AB4B
  border-bottom 2px solid #59AB4B

h1
  margin 0 5px
  font-family 'TommasoRegular'
  text-rendering optimizeLegibility
  font-size 85px
  font-weight normal
  text-align center

h1 span.a
  margin-right -5px

h1 span.y
  margin-left -3px

h1 span.hyphen
  display inline-block
  margin 0 -9px
  transform scale(.5,1)
  -ms-transform scale(.5,1)
  -webkit-transform scale(.5,1)
  -o-transform scale(.5,1)
  -moz-transform scale(.5,1)

.red
  color #D64622!important
  text-decoration none
  a
    color #D64622!important

.green
  color #59AB4B!important
  text-decoration none

#wrapper
  width 925px
