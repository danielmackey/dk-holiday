#container #video span
  display block
  position relative
  z-index 10
  padding-top 16px
  text-align center
  text-transform uppercase
  font-weight bold
  font-size 12px
  letter-spacing 1px

.circle
  width 160px
  height 80px
  display block
  position absolute
  z-index 10
  top 150px
  margin-left -40px
  color #737364
  text-transform uppercase
  line-height 35px
  font-weight bold
  text-decoration none
  cursor pointer
  text-align center

.circle span
  color #737364

.circle span.large
  display block
  position relative
  color #fff
  font-size 33px
  margin-top -3px
  font-family 'TommasoRegular'

.circle span.inner-circle
  width 160px
  height 160px
  display block
  background #C2C2A9
  top -129px
  z-index -12
  position relative
  -webkit-border-radius 80px 80px 80px 80px
  -moz-border-radius 80px 80px 80px 80px
  -ms-border-radius 80px 80px 80px 80px
  border-radius 80px 80px 80px 80px
  -webkit-transition .1s ease-in-out all
  -moz-transition .1s ease-in-out all
  -ms-transition .1s ease-in-out all
  transition .1s ease-in-out all

.circle.github
  margin-left 816px
  top 380px
  cursor pointer
  -webkit-transform rotate(-8deg)
  -moz-transform rotate(-8deg)
  -ms-transform rotate(-8deg)
  transform rotate(-8deg)
  -webkit-transition .1s ease-in-out all
  -moz-transition .1s ease-in-out all
  -ms-transition .1s ease-in-out all
  transition .1s ease-in-out all

.circle.scrooge
  -webkit-transform rotate(8deg)
  -moz-transform rotate(8deg)
  -ms-transform rotate(8deg)
  transform rotate(8deg)
  -webkit-transition .1s ease-in-out all
  -moz-transition .1s ease-in-out all
  -ms-transition .1s ease-in-out all
  transition .1s ease-in-out all

.circle.scrooge:hover
  -webkit-transform rotate(-8deg)
  -moz-transform rotate(-8deg)
  -ms-transform rotate(-8deg)
  transform rotate(-8deg)

.circle.github:hover
  color #7C2413
  cursor pointer
  -webkit-transform rotate(8deg)
  -moz-transform rotate(8deg)
  -ms-transform rotate(8deg)
  transform rotate(8deg)

.circle.scrooge:hover span
  color #2A6130

.circle.scrooge:hover span.large
  color #fff

.circle.github:hover span
  color #7C2413

.circle.github:hover span.large
  color #fff

.circle.scrooge:hover span.inner-circle
  background #59AB4B
  cursor pointer
  -webkit-transform scale(1.1,1.1) rotate(8deg)
  -moz-transform scale(1.1,1.1) rotate(8deg)
  -ms-transform scale(1.1,1.1) rotate(8deg)
  transform scale(1.1,1.1) rotate(8deg)

.circle.github:hover span.inner-circle
  background #D64622
  cursor pointer
  -webkit-transform scale(1.1,1.1) rotate(8deg)
  -moz-transform scale(1.1,1.1) rotate(8deg)
  -ms-transform scale(1.1,1.1) rotate(8deg)
  transform scale(1.1,1.1) rotate(8deg)

