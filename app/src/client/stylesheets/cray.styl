#container #video
  width 670px
  position relative
  z-index 1
  height 343px
  overflow hidden

  iframe
    position absolute
    top 0px
    left 0
    z-index 1

  #top-rail
    height 45px
    position absolute
    width 100%
    top 0
    left 0
    z-index 2
    background url('/images/top-rail.png') no-repeat

  #left-rail
    height 100%
    width 30px
    position absolute
    top 30px
    left 0
    z-index 2
    background url('/images/left-rail.png') no-repeat

  #bottom-rail
    height 30px
    width 100%
    position absolute
    bottom -13px
    right -15px
    z-index 2
    background url('/images/bottom-rail.png') no-repeat

  #right-rail
