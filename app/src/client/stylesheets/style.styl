@import reset
@import cray
@import tip

.tpl
  display none


#overlay
  position:fixed
  top 0
  left 0
  width 100%
  height 100%
  z-index 10000
  display none

  #shade
    opacity 0.7
    background #737364
    position absolute
    top 0
    left 0
    height 100%
    width 100%

  #holidays-past
    position relative
    height 480px
    width 690px
    padding 20px
    background #FFFFF5
    margin 100px auto
    padding 1px 0 0

    h2
      float left
      margin 20px 0 0 20px
      font-size 20px

    ul
      float right

      li
        height auto
        float left
        margin 20px 20px 0 0

        a
          font-weight bold
          font-size:20px
          cursor pointer
          text-decoration underline
          color #59AB4B
        a:hover
          color #2A6130

    #video
      margin 60px 0 0 20px



