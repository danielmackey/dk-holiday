@import reset
@import cray

.tpl
  display none


#overlay
  position:fixed
  top 0
  left 0
  width 100%
  height 100%
  z-index 10000
  display none

  #shade
    opacity 0.7
    background #000
    position absolute
    top 0
    left 0
    height 100%
    width 100%

  #holidays-past
    position relative
    height 450px
    width 650px
    background #000
    margin 100px auto
    padding 1px 0 0

    h2
      float left
      margin 20px 0 0 20px

    ul
      float right

      li
        height auto
        float left
        margin 20px 20px 0 0

        a
          font-weight bold
          cursor pointer

    #video
      margin-top 60px
